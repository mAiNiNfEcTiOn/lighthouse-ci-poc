{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "schemas/*.js",
    "index.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 88,
      "functions": 72,
      "lines": 75,
      "statements": 75
    },
    "./schema/**/*.js": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "moduleFileExtensions": ["js"],
  "watchman": false
}
